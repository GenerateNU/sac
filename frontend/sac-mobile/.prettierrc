{
    "plugins": [
      "@trivago/prettier-plugin-sort-imports"
    ],
    "printWidth": 80,
    "tabWidth": 4,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "none",
    "bracketSpacing": true,
    "arrowParens": "always",
    "endOfLine": "auto",
    "importOrder": [
        "^react",
        "^react-native",
        "^expo",
        "<THIRD_PARTY_MODULES>",
        "^@/",
        "^src",
        "^[./]"
      ],
      "importOrderSeparation": true,
      "importOrderSortSpecifiers": true
}
